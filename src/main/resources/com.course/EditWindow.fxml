<?import javafx.scene.control.*?>

<MenuBar xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
         fx:controller="com.course.frontEnd.controllers.MainWindowController">
    <menus>
        <Menu mnemonicParsing="false" text="File">
            <items>
                <MenuItem mnemonicParsing="false" onAction="#newAction" text="New"/>
                <MenuItem mnemonicParsing="false" onAction="#openAction" text="Open"/>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem mnemonicParsing="false" onAction="#saveAction" text="Save"/>
                <MenuItem mnemonicParsing="false" onAction="#saveAsAction" text="Save as..."/>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem mnemonicParsing="false" onAction="#exitAction" text="Exit"/>
            </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Edit">
            <items>
                <MenuItem mnemonicParsing="false" onAction="#addElementAction" text="Add element"/>
                <MenuItem mnemonicParsing="false" onAction="#deleteElementAction" text="Delete element"/>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem mnemonicParsing="false" onAction="#addTableAction" text="Add table"/>
                <MenuItem mnemonicParsing="false" onAction="#deleteTableAction" text="Delete table"/>
            </items>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
            <items>
                <MenuItem mnemonicParsing="false" onAction="#helpAction" text="Help"/>
                <SeparatorMenuItem mnemonicParsing="false"/>
                <MenuItem mnemonicParsing="false" onAction="#aboutAction" text="About"/>
            </items>
        </Menu>
    </menus>
</MenuBar>